#include "LCD.h"

uint8_r lcdData = 0;

void LCD_sendData(uint8_t data){
	LCD_E_High();

	// High 4bit
	lcdData = (data & 0xf0) | (lcdData & 0x0f);
	LCD_sendI2C(lcdData);

	LCD_E_Low();
	// Low 4bit
	data = data << 4;
	lcdData = (data & 0xf0) | (lcdData & 0x0f);
	LCD_sendI2C(lcdData);
}

LCD_sendI2C(uint8_t data) {

}
