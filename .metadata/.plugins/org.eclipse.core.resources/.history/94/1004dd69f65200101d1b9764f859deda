#include "Presenter.h"
#include <string.h>
#include <stdio.h>
#include "usart.h"

static char LCD_out[18] = "xxxx";
static uint32_t FND_out = 0;

void Presenter_OutData();
void Presenter_DisTimeWatchFnd();

void Presenter_OutData()
{

}

void Presenter_DisTimeWatchFnd(){
	FND_WriteData(FND_out);
	FND_WriteDp(FND_DP_1000|FND_DP_10, FND_DP_OFF);
}

///// public /////
void Presenter_Excute()
{
	Presenter_OutData();
	Presenter_DispTimeWatch();
	LCD_WriteStringXY(0,0,LCD_out);
}

void Presenter_Init() {
	LCD_Init(&hi2c1);
}
