#include "TimeWatch.h"

typedef struct timeWatch{
	uint16_t msec;
	uint8_t sec;
	uint8_t min;
	uint8_t hour;
}timeWatch_t;

timeWatch_t timeWatch = {0,0,0,0};

void TimeWatch_IncTimeCallBack() {
	if(timeWatch.msec < 1000) {
		timeWatch.msec++;
		return;
	}
	timeWatch.msec = 0;

	if(timeWatch.sec < 60 - 1) {
		timeWatch.sec++;
		return;
	}
	timeWatch.sec = 0;

	if(timewatch.min < 60 - 1) {
		timeWatch.min++;
		return;
	}
}

