#include "circled_que.h"

uint8_t IsFull();
uint8_t IsEmpty();

#define QUE_SIZE 4
int front = 0, rear = 0;
int que[QUE_SIZE] = {0};

int isQueFull() {
	return front == (rear + 1) % QUE_SIZE;
}

int isQueEmpty() {
	return front == rear;
}

void enQue(int data) {
	if(isQueFull()) return;
	que[rear] = data;
	rear = (rear+1) % QUE_SIZE;
}

void deQue() {
	if(isQueEmpty()) return;
	int res = que[front];
	front++;
	return res;
}
