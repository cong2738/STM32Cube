#ifndef DRIVER_FND_FND_H_
#define DRIVER_FND_FND_H_
#include <stdint.h>
#include "stm32f4xx.h"
#include "GPIO.h"

void FND_Init();
void FND_WriteDotData(uint16_t data);
void FND_WriteData(uint16_t data);
uint16_t FND_ReadData();
void FND_DispData();
void FND_WriteClock(uint8_t hour, uint8_t min);
void FND_WriteStopwatch(uint8_t min, uint8_t sec, uint32_t msec);


#endif /* DRIVER_FND_FND_H_ */
